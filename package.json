{
  "name": "containerbase",
  "version": "0.0.0-semantic-release",
  "private": true,
  "repository": "https://github.com/containerbase/base.git",
  "license": "MIT",
  "author": "Rhys Arkins <rhys@arkins.net>",
  "contributors": [
    "Michael Kriese <michael.kriese@visualon.de>"
  ],
  "type": "module",
  "scripts": {
    "bats": "node tools/bats.js --timing --verbose-run",
    "build": "run-s 'build:*'",
    "build:cli": "node tools/esbuild.js",
    "eslint": "eslint --cache .",
    "eslint-fix": "eslint --cache --fix .",
    "lint": "run-s prettier eslint lint:types",
    "lint:types": "run-p 'lint:types:*'",
    "lint:types:default": "tsc",
    "lint:types:dist": "tsc -p tsconfig.dist.json",
    "lint-fix": "run-s prettier-fix eslint-fix",
    "prepare": "husky",
    "prepare:proxy": "node tools/prepare-proxy.js",
    "prettier": "prettier --cache -c -u \"**/*.*\"",
    "prettier-fix": "prettier --cache -w -u \"**/*.*\"",
    "release:prepare": "node tools/prepare-release.js",
    "release:publish": "node tools/publish-release.js",
    "start": "tsx src/cli/index.ts",
    "test": "run-s 'test:*'",
    "test:bats": "node tools/bats.js --timing --verbose-run test/bash/ test/bash/v2",
    "test:docker": "node tools/test.js",
    "test:vitest": "vitest run --coverage"
  },
  "resolutions": {
    "bats-support": "0.3.0",
    "esbuild": "0.25.3",
    "tsconfig-paths": "4.2.0",
    "vite": "6.3.4"
  },
  "dependencies": {
    "@renovatebot/pep440": "4.1.0",
    "@sindresorhus/is": "7.0.1",
    "clipanion": "3.2.1",
    "common-tags": "1.8.2",
    "deepmerge": "4.3.1",
    "del": "8.0.0",
    "execa": "9.5.2",
    "global-agent": "3.0.0",
    "got": "14.4.7",
    "ini": "5.0.0",
    "inversify": "6.2.2",
    "pino": "9.6.0",
    "pino-pretty": "13.0.0",
    "pretty-ms": "9.2.0",
    "reflect-metadata": "0.2.2",
    "semver": "7.7.1",
    "simple-git": "3.27.0",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@containerbase/eslint-plugin": "1.1.4",
    "@eslint/js": "9.23.0",
    "@semantic-release/exec": "7.0.3",
    "@tsconfig/node20": "20.1.5",
    "@tsconfig/strictest": "2.0.5",
    "@types/common-tags": "1.8.4",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/global-agent": "3.0.0",
    "@types/ini": "4.1.1",
    "@types/node": "22.14.1",
    "@types/semver": "7.7.0",
    "@types/shelljs": "0.8.15",
    "@vitest/coverage-v8": "3.1.2",
    "@vitest/eslint-plugin": "1.1.43",
    "@vitest/ui": "3.1.2",
    "@yao-pkg/pkg": "6.4.0",
    "bats": "1.11.1",
    "bats-assert": "2.0.0",
    "bats-support": "0.3.0",
    "clipanion": "3.2.1",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "esbuild": "0.25.3",
    "esbuild-plugin-pino": "2.2.2",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-formatter-gha": "1.5.2",
    "eslint-import-resolver-typescript": "4.2.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-promise": "7.2.1",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "nock": "14.0.4",
    "npm-run-all2": "7.0.2",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "2.5.10",
    "semantic-release": "24.2.3",
    "shelljs": "0.9.2",
    "tsx": "4.19.3",
    "type-fest": "4.40.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.27.0",
    "vite": "6.3.4",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.2"
  },
  "packageManager": "pnpm@10.9.0",
  "engines": {
    "node": "^20.9.0 || ^22.11.0",
    "pnpm": "^10.0.0"
  },
  "pnpm": {
    "allowNonAppliedPatches": true,
    "supportedArchitectures": {
      "cpu": [
        "x64",
        "arm64"
      ],
      "os": [
        "linux",
        "win32"
      ]
    },
    "patchedDependencies": {
      "@yao-pkg/pkg": "patches/@yao-pkg__pkg.patch",
      "clipanion@3.2.1": "patches/clipanion@3.2.1.patch",
      "global-agent": "patches/global-agent.patch"
    }
  }
}
