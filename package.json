{
  "name": "containerbase",
  "version": "0.0.0-semantic-release",
  "private": true,
  "repository": "https://github.com/containerbase/base.git",
  "license": "MIT",
  "author": "Rhys Arkins <rhys@arkins.net>",
  "contributors": [
    "Michael Kriese <michael.kriese@visualon.de>"
  ],
  "type": "module",
  "scripts": {
    "bats": "node tools/bats.js --timing --verbose-run",
    "build": "run-s 'build:*'",
    "build:cli": "node tools/esbuild.js",
    "eslint": "eslint --cache .",
    "eslint-fix": "eslint --cache --fix .",
    "lint": "run-s prettier eslint lint:types lint:markdown",
    "lint:markdown": "markdownlint-cli2 '**/*.md'",
    "lint:types": "run-p 'lint:types:*'",
    "lint:types:default": "tsc",
    "lint:types:dist": "tsc -p tsconfig.dist.json",
    "lint-fix": "run-s prettier-fix eslint-fix 'lint-fix:*'",
    "lint-fix:markdown": "markdownlint-cli2 --fix '**/*.md'",
    "prepare": "husky",
    "prepare:proxy": "node tools/prepare-proxy.js",
    "prettier": "prettier --cache -c -u \"**/*.*\"",
    "prettier-fix": "prettier --cache -w -u \"**/*.*\"",
    "release:prepare": "node tools/prepare-release.js",
    "release:publish": "node tools/publish-release.js",
    "start": "tsx src/cli/index.ts",
    "test": "run-s 'test:*'",
    "test:bats": "node tools/bats.js --timing --verbose-run test/bash/ test/bash/v2",
    "test:docker": "node tools/test.js",
    "test:vitest": "vitest run --coverage"
  },
  "resolutions": {
    "bats-support": "0.3.0",
    "esbuild": "0.27.3",
    "tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@renovatebot/pep440": "4.2.1",
    "@seald-io/nedb": "4.1.2",
    "@sindresorhus/is": "7.2.0",
    "clipanion": "3.2.1",
    "common-tags": "1.8.2",
    "deepmerge": "4.3.1",
    "del": "8.0.1",
    "execa": "9.6.1",
    "global-agent": "3.0.0",
    "got": "14.6.6",
    "ini": "6.0.0",
    "inversify": "7.11.0",
    "nano-spawn": "2.0.0",
    "node-ipc": "12.0.0",
    "pino": "10.3.1",
    "pino-pretty": "13.1.3",
    "pretty-ms": "9.3.0",
    "reflect-metadata": "0.2.2",
    "semver": "7.7.4",
    "simple-git": "3.31.1",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@containerbase/eslint-plugin": "1.1.32",
    "@containerbase/istanbul-reports-html": "1.1.31",
    "@eslint/js": "9.39.2",
    "@semantic-release/exec": "7.1.0",
    "@tsconfig/node24": "24.0.4",
    "@tsconfig/strictest": "2.0.8",
    "@types/common-tags": "1.8.4",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/global-agent": "3.0.0",
    "@types/ini": "4.1.1",
    "@types/node": "24.10.13",
    "@types/node-ipc": "9.2.3",
    "@types/semver": "7.7.1",
    "@types/shelljs": "0.10.0",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/eslint-plugin": "1.6.9",
    "@vitest/ui": "4.0.18",
    "@yao-pkg/pkg": "6.14.0",
    "bats": "1.13.0",
    "bats-assert": "2.2.4",
    "bats-support": "0.3.0",
    "clipanion": "3.2.1",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "esbuild": "0.27.3",
    "esbuild-plugin-pino": "2.3.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-formatter-gha": "2.0.1",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-promise": "7.2.1",
    "globals": "17.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "markdownlint-cli2": "0.21.0",
    "nock": "14.0.11",
    "npm-run-all2": "8.0.4",
    "prettier": "3.8.1",
    "prettier-plugin-packagejson": "3.0.0",
    "semantic-release": "25.0.3",
    "shelljs": "0.10.0",
    "tsx": "4.21.0",
    "type-fest": "5.4.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.0",
    "vite-tsconfig-paths": "6.1.1",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "markdownlint": "*",
    "vite": "*"
  },
  "packageManager": "pnpm@10.30.0",
  "engines": {
    "node": "^22.11.0 || >=24.10.0",
    "pnpm": "^10.0.0"
  },
  "pnpm": {
    "supportedArchitectures": {
      "cpu": [
        "x64",
        "arm64"
      ],
      "os": [
        "linux",
        "win32",
        "darwin"
      ]
    },
    "overrides": {
      "verbose": "-"
    },
    "patchedDependencies": {
      "@semantic-release/github": "patches/@semantic-release__github.patch",
      "@yao-pkg/pkg": "patches/@yao-pkg__pkg.patch",
      "clipanion@3.2.1": "patches/clipanion@3.2.1.patch",
      "global-agent": "patches/global-agent.patch",
      "nano-spawn": "patches/nano-spawn.patch"
    }
  }
}
